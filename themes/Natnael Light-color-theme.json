{
	"name": "Natnael Light",
	"colors": {
		"editor.background": "#f0eded",
		"editor.foreground": "#303030",
		"list.activeSelectionIconForeground": "#f0f0f0",
		"statusBar.background": "#9b154f",
		"statusBarItem.remoteBackground": "#3e051e",
		"sideBar.background": "#f0eded",
		"sideBar.foreground": "#3e051e",
		"sideBar.border": "#b6b1b3",
		"activityBar.background": "#461328",
		"activityBar.activeBackground": "#89093e",
		"activityBar.foreground": "#d2baba",
		"activityBar.inactiveForeground": "#b2a1a8",
		"menu.foreground": "#3e051e",
		"icon.foreground": "#3e051e",
		"foreground": "#3e051e",
		"editorLineNumber.foreground": "#99878f",
		"editorLineNumber.activeForeground": "#492e3a",
		"panel.background": "#f0eded",
		"tab.hoverBackground": "#f0eded",
		"badge.background": "#f0eded",
		"banner.background": "#f0eded",
		"menu.background": "#f0eded",
		"gauge.background": "#f0eded",
		"editorPane.background": "#f0eded",
